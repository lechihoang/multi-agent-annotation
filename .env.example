# Groq API Settings (FREE tier, fast GPU inference)
# Get your API key: https://console.groq.com/
GROQ_API_KEY=your_key_here
GROQ_MODEL=llama-3.3-70b-versatile
GROQ_TEMPERATURE=0.1
GROQ_MAX_TOKENS=512

# HuggingFace (for local embeddings - retrieval agent)
HF_EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Agent Weights (MAFA 4-agent ensemble)
WEIGHT_PRIMARY=0.25
WEIGHT_CONTEXTUAL=0.25
WEIGHT_RETRIEVAL=0.25
WEIGHT_HYBRID=0.25

# Confidence Thresholds
THRESHOLD_APPROVE=0.85
THRESHOLD_REVIEW=0.60
THRESHOLD_ESCALATE=0.0

# No hardcoded labels - Tier 1 RouterAgent auto-infers task type and labels
# Labels are dynamically generated based on input data analysis
